<h1>New group</h1>

<%= semantic_form_for(@participant_group) do |f| %>

	<%= default_form_errors(f) %>

	<%= render partial: "fields", locals: { f: f } %>

	<%= f.submit "Create", disable_with: 'Creating...' %>
	<%= separator %>
	<%= link_to 'Cancel', participant_groups_path, class: '' %>

<% end %>
