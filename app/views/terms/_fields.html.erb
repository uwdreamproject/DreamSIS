<% f.inputs do %>
	<%= f.input :title unless f.object.is_a?(Quarter) %>
	<%= f.input :start_date %>
	<%= f.input :end_date %>
	<%= f.input :course_ids, :as => :text, :label => "Primary Course ID(s)", :input_html => { :rows => 3 }, 
					:hint => "Specify the course ID's for this term in the format: 2011,spring,EDUC,360/A. One per line." if Customer.link_to_uw? %>
	<%= f.input :allow_signups %>
<% end -%>