<h1>Editing request</h1>

<% semantic_form_for(@clearinghouse_request) do |f| %>
  	<%= f.error_messages %>
	<%= render :partial => "fields", :locals => { :f => f } %>

	<%- f.buttons do -%>
	 	<%= f.submit "Update" %>
		<%= separator %>
		<%= link_to 'Back', @clearinghouse_request, :class => 'cancel button' %>
	<% end -%>

<% end %>