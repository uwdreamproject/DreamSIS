<% content_tag('ul') do %>
	<% for result in auto_complete_location_name %>
		<li id="<%= result.class.to_s.underscore %>_<%= result.id %>" class="icon-left <%= result.class.to_s.underscore %>">
			<span class="primary"><%= highlight(h(result.name), h(highlight_phrase)) %></span>
			<span class="secondary"><%=h result.type.to_s.titleize %></span>
		</li>
	<% end -%>	
<% end -%>
