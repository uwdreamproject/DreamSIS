<%= breadcrumbs :participants, @participant %>
<%= operations :save %>

<%= semantic_form_for(@participant, html: {id: "participant_form", multipart: true}) do |f| %>
	<%= default_form_errors(f) %>

	<%= render partial: 'participants/fields/name_and_personal', locals: { f: f } %>
	<%= render partial: 'participants/fields/contact', locals: { f: f } %>
	<%= render partial: 'participants/fields/high_school', locals: { f: f } %>
	<%= render partial: 'participants/fields/background', locals: { f: f } %>
	<%= render partial: 'participants/fields/race_ethnicity', locals: { f: f } %>
	<%= render partial: 'participants/fields/family_history', locals: { f: f } %>
	<%= render partial: 'participants/fields/financial', locals: { f: f } %>
	<%= render partial: 'participants/fields/parents', locals: { f: f } %>
	<%= render partial: 'participants/fields/administrative', locals: { f: f } %>
	<%#= render partial: 'participants/fields/post_program', locals: { f: f } %>

	<%= default_form_actions(f) %>
<% end %>
