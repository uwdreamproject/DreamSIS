<h1>New event type</h1>

<% semantic_form_for(@event_type) do |f| %>

	<%= f.error_messages %>

	<%= render :partial => "fields", :locals => { :f => f } %>

	<%= f.submit "Create", :disable_with => 'Creating...' %>
	<%= separator %>
	<%= link_to 'Cancel', event_types_path, :class => '' %>

<% end %>
