<h1>Editing event type</h1>

<%= semantic_form_for(@event_type, :url => event_type_path(@event_type)) do |f| %>

	<%= f.semantic_errors %>

	<%= render :partial => "fields", :locals => { :f => f } %>

	<%= f.submit "Update", :disable_with => 'Updating...' %>
	<%= separator %>
	<%= link_to 'Cancel', event_types_path, :class => '' %>

<% end -%>
