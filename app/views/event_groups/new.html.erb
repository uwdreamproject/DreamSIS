<h1>New event group</h1>

<%= semantic_form_for(@event_group) do |f| %>

	<%= default_form_errors(f) %>

	<%= render :partial => "fields", :locals => { :f => f } %>

	<%= f.submit "Create", :disable_with => 'Creating...' %>
	<%= separator %>
	<%= link_to 'Cancel', event_groups_path, :class => '' %>

<% end %>
