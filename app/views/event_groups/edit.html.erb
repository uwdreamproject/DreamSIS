<h1>Editing event group</h1>

<%= semantic_form_for(@event_group, url: event_group_path(@event_group)) do |f| %>

	<%= default_form_errors(f) %>

	<%= render partial: "fields", locals: { f: f } %>

	<%= f.submit "Update", disable_with: 'Updating...' %>
	<%= separator %>
	<%= link_to 'Cancel', event_groups_path, class: '' %>

<% end -%>
