  background-repeat: no-repeat;
  margin-right: 1em;
  margin-left: -1.9em;
  padding-left: 1.9em;
}

.filters li input[value=true].enabled ~ span {
	background-image: url(<%= asset_path 'icons/silk/tick.png' %>);
}

.filters li input[value=false].enabled ~ span {
	background-image: url(<%= asset_path 'icons/silk/cross.png' %>);
}

