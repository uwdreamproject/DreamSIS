.arrow-box {
	position: relative;
	border: none;
	background: white;
  border: none;
	padding: 0.25em;
	border-radius: 3px;
  box-shadow: 0px 0px 4px rgba(0,0,0,0.3);
}
.arrow-box:after, .arrow-box:before {
  top: 100%;
	left: 50%;
	border: solid transparent;
	content: " ";
	height: 0;
	width: 0;
	position: absolute;
	pointer-events: none;  
}
.arrow-box:after {
	border-top-color: #ffffff;
	border-width: 7px;
	margin-left: -7px;
}
.arrow-box:before {
  border-top-color: rgba(0,0,0,0.1);
	border-width: 8px;
	margin-left: -8px;
}

.arrow-box.arrow-top:after, .arrow-box.arrow-top:before {
  bottom: 100%;
  top: auto;
}

.arrow-box.arrow-top:after {
	border-top-color: transparent;
	border-bottom-color: #ffffff;
}

.arrow-box.arrow-top:before {
  border-top-color: transparent;
  border-bottom-color: rgba(0,0,0,0.1);
}


