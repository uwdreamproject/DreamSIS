
@import url(https://fonts.googleapis.com/css?family=Oxygen:400,300,700);


html {
	margin: 0;
	padding: 0;
}

body {
/*  background: #4b4b4b;*/
	margin: 0;
	padding: 0;
	text-align: left;
	font: 13px/130% "Helvetica Neue", Helvetica, Arial, sans-serif;
	-webkit-font-smoothing: antialiased;
	z-index: 0;
  
  -webkit-background-clip: border-box;
  -webkit-background-origin: padding-box;
  -webkit-background-size: auto;
  background-attachment: scroll;
  background-clip: border-box;
  background-color: rgb(238,238,238);
  background-image: -webkit-gradient(linear, 0% 0%, 0% 100%, color-stop(0.03, rgb(255, 255, 255)), color-stop(0.2, rgb(238, 238, 238)));
  background-origin: padding-box;
  background-size: auto;
  background-repeat: no-repeat;
}

h1, h2, h3, h4, h5 {
	font-family: 'Oxygen', 'Lucida Grande', 'Lucida Sans Unicode', Arial, sans-serif;
	color: #39275B;
	font-style: normal;
	font-weight: normal;
	line-height: 130%;
}

h1 {
	font-size: 200%;
	color: #323131;
	font-weight: 600;
	line-height: 100%;
}

h2 {
	font-size: 100%;
  text-transform: uppercase;
  font-weight: 800;
}

h3 {
	font-size: 90%;
  text-transform: uppercase;
  font-weight: 600;
}

h3.pre-title {
	text-transform: uppercase;
	color: #979798;
	margin: 0;
	margin-bottom: 0.25em;
	font-weight: bold;
	letter-spacing: 1px;
	font-size: 90%;
	font-weight: normal;
}

h3.pre-title a {
	text-decoration: none;
}

h3.pre-title a:hover {
	text-decoration: underline;
}


h4, h5 {
	font-size: 100%;
}

h5 {
	font-family: Helvetica, Arial, sans-serif;
	font-weight: bold;
}

a {
	text-decoration: underline;
	color: #036;
}

a:hover {
	text-decoration: none;
}

a:visited {
	color: #036;
}

.clear {
	clear: both;
}

.weak {
  font-weight: normal !important;
}

.activity_logs.weekly_summary .highlight_container {
	clear: left;
	padding-bottom: 1em;
	min-height: 60px;
	border-bottom: 1px solid #efefef;
}

.activity_logs.weekly_summary .highlight_container h4 {
	font-weight: bold;
}

.activity_logs.weekly_summary blockquote {
	margin-left: 65px;
	margin-right: 0;
}

.activity_logs.weekly_summary blockquote p:first-child {
	margin-top: 0;
}

.activity_logs.weekly_summary .photo {
	float: left;
	margin-right: 1em;
}

.handle {
  color: rgba(0,0,0,0.35);
}

.handle:hover {
  cursor: move;
}

table {
	width: 100%;
	border-collapse: collapse;
	background: white;
}

tr {
	/*transition: background-color 0.15s ease-in-out;*/
}

table.multirow tbody:hover td.multirow-highlight {
	background: #fffde7;
}

tr.odd {
	background: #fafbfc;
}

th, td {
	padding: 4px;
	margin: 0;
}

th {
	text-align: left;
	border-bottom: 1px solid rgb(176, 183, 226);
	background-color: rgb(246, 248, 252);
	padding: 10px 4px;
}

td {
	border-bottom: 1px solid #eee;
	color: #424242;
}

tr:hover, tr.odd:hover, tr.even:hover, tr.highlight {
	background: #fffde7;
}

/*tr:hover td, tr.odd:hover td, tr.even:hover td, tr.highlight td {
	background: #fffde7;
}
*/

table tr:not(.not-first-child) td:first-child, table th:first-child {
	padding-left: 30px !important;
}

table td:last-child, table th:last-child {
	padding-right: 30px !important;
}

table.borderless td {
  border-bottom: none !important;
}

tr.rowset-header td {
	font-weight: bold;
	background: #e5e5e5;
	padding-top: 5px !important;
}

table.participants td {
	padding: 0.6em 4px;
}

table.participants td a {
	text-decoration: none;
}

table td.name a {
	font-weight: bold;
	text-decoration: none;
}

table.participants td.name {
	font-weight: bold;
}

table.participants td.name > a {
    max-width: 200px;
    display: block;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

table.participants img.avatar {
	width: 32px;
	height: 32px;
	float: left;
	margin-right: 1em;
	border: 1px solid #e0e0e0;
	border-radius: 100px;
}

table.participants td.college-logo {
  padding-left: 0 !important;
}

table.participants.loading {
	background: url(<%= asset_path 'bar-loader.gif' %>) no-repeat center 25%;
}

.loading {
	background: url(<%= asset_path 'bar-loader.gif' %>) center 75% no-repeat;
	min-height: 5em;
	min-width: 220px;
	padding: 6px;
	display: block;
}

table.participants td, table.participants th {
	transition: opacity 0.25s ease-in-out 0s;
	transition: color 0.25s ease-in-out 0s;
}

table.participants.loading th:not(.name) {
	color: transparent;
}

table.participants.loading td {
	opacity: 0;
}

table.participants.loading td.name {
	opacity: 1;
}

table.participants.active-report td {
  vertical-align: top;
}

.last-updated.old {
  color: rgba(255,0,0,0.75);
}

.gain_or_loss {
	font-weight: bold;	
}

.gain_or_loss.up {
	color: green;
}

.gain_or_loss.down {
	color: red;
}

.gain_or_loss.up:before {
	content: '+ ';
}

table.users td.login {
	font-weight: bold;
}

table.users td.login a {
	text-decoration: none;
}

table.locations td.name {
	font-weight: bold;
}

table.locations td.name a {
	text-decoration: none;
}

table td.functions {
	text-align: right;
	width: 10px;
	white-space: nowrap;
}

table td.functions a:not(.default) {
	font-size: 90%;
	display: inline-block;
	padding: 4px 10px;
	text-decoration: none;
	border: 1px outset #dadada;
	border-radius: 10px;
	background-color: #f9f9f9;
	cursor: default;
	color: rgb(60, 60, 60);
	
	/*border: 1px outset rgba(153,153,153,.5);*/
	border: none;
	border-radius: 4px;
	/*box-shadow: 0 1px 2px rgba(0,0,0,0.1);*/
	box-shadow: rgb(203, 210, 217) -1px -1px 0px 0px inset;
	
	
}

table td.functions a:active {
	/*border-style: inset;*/
	background-color: #f2f2f2;
	box-shadow: rgb(203, 210, 217) 1px 1px 0px 0px inset;

}

/*table td.functions a:not(.default) {
  visibility: hidden;
}

table tr:hover td.functions a:not(.default) {
  visibility: visible;
}
*/

table.quarter_status table td {
	border-bottom: none;
}

table.quarter_status td.status_tag {
	width: 7em;
	text-align: center;
}

table.quarter_status td.quarter_title {
	vertical-align: top;
	padding-top: 0.5em;
	font-weight: bold;
	text-transform: uppercase;
}

table.quarter_status td.section_id {
	width: 4em;
}

tr.total td {
	background: #f1f1f1 repeat-x 0 top;
	font-weight: bold;
}

tr.merge_source {
	background-color: #f6a59c;
}

tr.merge_target {
	background-color: #ace1a4;
}

#merge_form .merge_item {
	padding: 0.5em;
	border: 2px dashed #dddcdc;
}

#merge_form p.name {
	margin-bottom: 0;
}

#merge_form p strong {
	display: block;
	font-size: 120%;
	line-height: 1.3em;
}

#merge_form label {
	font-size: 85%;
}

#merge_form em.separator {
	display: block;
	margin: 1em auto;
	text-align: center;
}

#merge_form p.error-message, #merge_form p.success-message {
	font-weight: normal;
	font-size: 85%;
	line-height: 1.2em;
	margin: -6px;
	margin-bottom: 0.5em;
	padding: 1em;
	border-bottom: 1px inset #e2e2e2;
	background: #ececec;
}

#merge_form em.separator:before {
	content: '\2193  ';
	padding-right: 1em;
}

#merge_form em.separator:after {
	content: '  \2193';
	padding-left: 1em;
}


#merge_form input:not([type=submit]):not(.editable) {
	width: 2em;
	background: none;
	display: inline;
	padding: 0;
	border: none;
	box-shadow: none;
	font-size: 75%;
}

h4 {
	margin: 0;
	color: #81a4cc;
	margin-top: 10px;
}

#indicator {
	position: fixed;
	top: -6px;
	left: 50%;
	width: 70px;
	margin-left: -45px;
	background: yellow url(<%= asset_path 'indicator.gif' %>) no-repeat 8px 10px;
	padding: 10px 10px 5px 30px;
	border-radius: 5px;
	font-weight: bold;
	opacity: 0;
	text-shadow: rgba(255,255,255,0.9) 0 1px 0;
	transition: all 0.25s ease-in-out;
	z-index: 1000;
}

#indicator.visible {
	opacity: 0.9;
}

#indicator .stat-meter {
  width: 200px;
  margin: 5px;
  left: -30px;
  display: none;
}

#indicator a.stop-loading {
  position: absolute;
  right: 10px;
  display: none;
  font-weight: normal;
}

#indicator .capacity, #indicator .size {
  height: 5px;
}

#indicator.progress {
  width: 173px;
  margin-left: -100px;
}

#indicator.progress .stat-meter {
  display: block;
}

#indicator.progress a.stop-loading {
  display: inline;
}

