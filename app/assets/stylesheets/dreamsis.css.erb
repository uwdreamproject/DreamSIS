ul.filters li ul { 
  transition: opacity 0.5s, height 0.5s;
  overflow: hidden;
}

ul.filters li.closed ul {
  opacity: 0;
  height: 0;
}

.filters li {
	clear: both;
	margin-bottom: 5px;
}

.filters > li.category > h4 {
	padding-top: 0.5em;
  font-weight: bold;
	text-transform: uppercase;
	font-size: 85%;
	font-family: "Oxygen", sans-serif;
  cursor: pointer;
}

