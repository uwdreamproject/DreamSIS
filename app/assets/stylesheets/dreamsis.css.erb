/* @group Autocomplete */

div.auto_complete {
  width: 350px;
  z-index: 100;
}

#sidebar div.auto_complete {
	text-shadow: none;
}

div#institution_name_auto_complete ul {
	width: 350px;
}

div#scholarship_title_auto_complete ul {
	width: 350px;
}

.ui-widget {
	font: 13px/130% "Helvetica Neue", Helvetica, Arial, sans-serif;
}

.ui-widget-content a {
  text-decoration: none;
}

ul.ui-autocomplete {
  border: 1px solid #9aa4ab;
  border-top: none;
  margin:0;
  margin-top: 0px;
  padding:0;
  width: 200px;
  list-style-type:none;
  background: #fff;
  position: absolute;
  left: 0;
}

.menu ul.ui-autocomplete {
	width: 100%;
	position: relative !important;
	display: block;
	margin-left: -7px;
	padding: 0;
	padding-right: 12px;
	left: 0;
	top: 0;
	z-index: 100;
	text-shadow: none;
}

.menu ul.ui-autocomplete li:hover {
	background: #415994;
	cursor: pointer;
}

.menu li.search {
	margin-left: -4px !important;
	padding-right: 0;
}

.menu li.search input.search {
	border: none;
	box-shadow: none;
	margin: 0;
	width: 100%;
	border-bottom: 1px dotted #dadada;
}

div.auto_complete.right ul {
	right: 0;
	left: auto;
}

ul.ui-autocomplete li {
  margin:0;
  padding: 6px;
  line-height: 100%;
  border-top: 1px solid #dadada;
}

ul.ui-autocomplete li.icon-left {
	padding-left: 30px !important;
	background-position: 5px;
}

ul.ui-autocomplete div.empty {
	padding: 0.5em;
	padding-left: 25px;
	text-align: left;
	background: url(<%= asset_path 'exclamation-mini.gif' %>) no-repeat 5px center;
	color: rgba(0,0,0,0.5);
}

.apply ul.ui-autocomplete li {
	padding-left: 30px;
	background: url(<%= asset_path 'icons/silk/page_white_text.png' %>) no-repeat 6px 6px;
}

div#organization_name_auto_complete ul li {
	padding-left: 30px;
	background: url(<%= asset_path 'icons/silk/building.png' %>) no-repeat 6px 6px;
}

div#institution_name_auto_complete ul li {
	background-image: url(<%= asset_path 'icons/silk/building.png' %>);
	background-repeat: no-repeat;
	background-position: 6px 6px;
}

ul.ui-autocomplete strong.highlight {
  margin:0;
  padding:0;
  background: #d8dfea;
}

ul.ui-autocomplete .primary, .primary {
	color: #000;
	margin-bottom: 0;
	display: block;
}

ul.ui-autocomplete .secondary, .secondary {
	font-weight: normal;
	font-size: 90%;
	color: #808080;
	display: block;
	margin-bottom: 0;
}

ul.ui-autocomplete .tertiary, .tertiary {
	font-weight: normal;
	font-size: 80%;
	color: #808080;
	display: block;
	margin-bottom: 0;
}

ul.ui-autocomplete li.selected,
div#organization_name_auto_complete ul li.selected {
  background-color: #415994;
  color: #fff;
}

ul.ui-autocomplete li.selected .primary {
	color: #fff;
}

ul.ui-autocomplete li.selected .secondary, ul.ui-autocomplete li.selected .tertiary {
	color: #d9d9d9;
}

ul.ui-autocomplete li.selected strong.highlight {
	background-color: #5670a6;
}

ul.ui-autocomplete .new {
	padding-top: 10px;
	font-size: 95%;
	background: url(<%= asset_path 'icons/silk/add.png' %>) no-repeat 6px center;
	padding-left: 30px;
	background-color: #d8dfea;
}

li.input ul.ui-autocomplete.after_select {
	margin-left: 25%;
}

/* @end */









