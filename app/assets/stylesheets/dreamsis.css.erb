td.signup-meter {
	padding-right: 0.5em;
	min-width: 3em;
}

.signup-meter {
	position: relative;
}

.signup-meter div.capacity {
	background: rgb(230,230,230);
	z-index: 10;
	float: left;
	border-radius: 5px;
/*  border: 1px solid rgba(0,0,0,0.05);*/
	background: linear-gradient(to bottom, #f8f8f8 0%, #f0f0f0 100%);
	overflow: hidden;
  height: 10px;
}

.signup-meter div.size {
	background: linear-gradient(to bottom, rgba(0, 150, 0, 0.1) 0%, rgba(0,150,0,0.5) 100%) rgba(0,150,0,0.5);
	z-index: 11;
	float: left;
	border-radius: 5px 0 0 5px;
  height: 10px;
  transition: all 0.25s ease-in-out;
}

.signup-meter div.target {
	z-index: 12;
	float: left;
	/*background: rgba(0,255,0,0.25);*/
	border-right: 1px dashed #999;
	position: absolute;
}

.signup-meter.full div.size {
	background: rgba(255,0,0,0.25);
}

.signup-meter .full_label {
	padding: 1px 5px;
	text-transform: uppercase;
	font: bold 80% "Helvetica Neue", Arial, Helvetica, Geneva, sans-serif;
	text-shadow: 0 1px 1px rgba(255,255,255,0.7);
	letter-spacing: 1px;
	color: #525252;
}

/* These settings are needed to override the generic div.warning settings above */
div.signup-meter.warning {
  background: none;  
  border: none;
  padding: 0;
  padding-left: 0;
  margin: 0;
}

.signup-meter.warning div.capacity {
	background: rgba(255,0,0,0.25);
	background: linear-gradient(to bottom, rgba(255,0,0,0.05) 0%, rgba(255,0,0,0.15) 100%);
}

.signup-meter.warning div.size {
	background: linear-gradient(to bottom, #ef8a7d 0%, red 100%) red;
}

.signup-meter.not_achieved div.size {
	background: linear-gradient(to bottom, #ffe898 0%, #ffcd19 100%) #ffcd19;
}

