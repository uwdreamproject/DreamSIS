.button.icon-only {
	width: 16px;
	height: 16px;
	padding: 0;
	margin: 0;
	background-color: none !important;
	border: none;
	box-shadow: none;
	background-position: 0 0;
	border-radius: 0;
	vertical-align: text-bottom;
	cursor: pointer;
	position: relative;
	text-shadow: none;
}

.button.icon-only:hover {
	/*box-shadow: 1px 1px 3px rgba(0,0,0,0.3);*/
}

.button.icon-only > span {
	display: none;
	position: absolute;
	left: -75%;
	top: -23px;
	font-size: 90%;
	background: rgba(0,0,0,0.7);
	color: white;
	text-shadow: 0px 1px 1px rgba(0,0,0,0.5);
	padding: 1px 5px;
	border-radius: 3px;
}

.button.icon-only:after {
	content: " ";
	display: none;
	position: absolute;
	top: -6px;
	width: 16px;
	height: 10px;
	background: url(<%= asset_path 'icons/down-arrow.png' %>) center 1px no-repeat;
}

.button.icon-only:hover > span {
	display: block;	
}

.button.icon-only:hover:after {
	display: block;
}

